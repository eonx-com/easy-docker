{
    "name": "eonx-com/easy-docker",
    "description": "CLI tool to generate Docker files",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "eonx-com/utils": "^1.0",
        "twig/twig": "^2.8",
        "symfony/filesystem": "^4.2",
        "symfony/finder": "^4.2",
        "symplify/package-builder": "^7.2",
        "symplify/set-config-resolver": "^7.2",
        "symplify/autowire-array-parameter": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyDocker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyDocker\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/easy-docker"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    }
}
