{
    "name": "eonx-com/easy-docker",
    "description": "CLI tool to generate Docker files",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "eonx-com/utils": "^0.2",
        "symplify/package-builder": "^6.1",
        "twig/twig": "^2.8",
        "symfony/filesystem": "^4.2",
        "symfony/finder": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "LoyaltyCorp\\EasyDocker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LoyaltyCorp\\EasyDocker\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/easy-docker"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.12-dev"
        }
    }
}
