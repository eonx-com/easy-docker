{
    "name": "eonx-com/easy-docker",
    "description": "CLI tool to generate Docker files",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "nette/utils": "^3.1",
        "twig/twig": "^2.8",
        "symfony/filesystem": "^4.2 || ^5.0",
        "symfony/finder": "^4.2 || ^5.0",
        "symplify/package-builder": "^8.2",
        "symplify/set-config-resolver": "^8.2",
        "symplify/autowire-array-parameter": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.4"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyDocker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyDocker\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/easy-docker"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.6-dev"
        },
        "eonx_docs": {
            "deprecated": "yes"
        }
    }
}
